<%= simple_form_for(@team) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :captain, :as => :hidden, input_html: { value: current_user.id } %>
    <%= f.association :game %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
